Options All -Indexes
#重写foto和tag页面
RewriteEngine  on
RewriteBase    /
#RewriteRule foto\.php\?pid=(\d+) foto/$1-?(.*)\.html           [NC,R=301]
RewriteRule foto/(\d+)-?(.*)\.html foto\.php\?pid=$1           [NC,L]
RewriteRule tag/(\d+)-?(.*)\.html tag\.php\?tagid=$1&tag=$2    [NC,L]
#错误页
ErrorDocument 404             /404.php
